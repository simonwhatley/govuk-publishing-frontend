{
  "name": "@simonwhatley/govuk-publishing-frontend",
  "description": "GOV.UK Publishing Frontend contains the code you need to start building user interfaces for UK government services. It is an amalgamation of components and patterns described in GOV.UK component guides.",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "pre-release": "node bin/check-nvmrc.js && ./bin/pre-release.sh",
    "release": "node bin/check-nvmrc.js && ./bin/release.sh",
    "release-to-branch": "node bin/check-nvmrc.js && ./bin/release-to-branch.sh",
    "test": "standard",
    "lint": "gulp lint:scss",
    "build:package": "node bin/check-nvmrc.js && gulp build:package --destination 'package'",
    "build:dist": "node bin/check-nvmrc.js && gulp build:dist --destination 'dist'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonwhatley/govuk-publishing-frontend.git"
  },
  "author": "Simon Whatley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simonwhatley/govuk-publishing-frontend/issues"
  },
  "homepage": "https://github.com/simonwhatley/govuk-publishing-frontend#readme",
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.20.3",
    "browser-sync": "^3.0.4",
    "cssnano": "^7.0.7",
    "del": "^3.0.0",
    "dotenv": "^17.1.0",
    "eslint": "^9.29.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "govuk-frontend": "^5.11.0",
    "gulp": "^5.0.1",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-better-rollup": "^4.0.1",
    "gulp-cache": "^1.1.3",
    "gulp-concat": "^2.6.1",
    "gulp-eol": "^0.2.0",
    "gulp-if": "^3.0.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-rename": "^2.1.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-lint": "^1.4.0",
    "gulp-uglify": "^3.0.2",
    "gulp-umd": "^2.0.0",
    "inquirer": "^12.6.3",
    "moment": "^2.30.1",
    "multer": "^1.4.2",
    "nunjucks": "^3.2.4",
    "oldie": "^1.3.0",
    "portscanner": "^2.2.0",
    "postcss-pseudo-classes": "0.4.0",
    "require-dir": "^1.0.0",
    "rollup": "^1.32.1",
    "sass": "^1.89.2",
    "standard": "^17.1.2",
    "vinyl-paths": "^3.0.1",
    "yargs": "^15.4.1"
  },
  "standard": {
    "ignore": [
      "/dist/**/*.js",
      "/package/**/*.js",
      "/src/govuk-pub/vendor/*.js"
    ]
  }
}
